	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"namor": "^2.0.2",
		"npm-watch": "^0.7.0",
		"react-scripts": "^4.0.3"
